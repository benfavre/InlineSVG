{
  "name": "jquery.inlineSVG",
  "version": "1.1.0",
  "homepage": "https://github.com/createlogic/InlineSVG",
  "authors": [
    "Bilal Niaz Awan <createlogic@gmail.com>",
    "Viacheslav Lotsmanov <lotsmanov89@gmail.com>"
  ],
  "description": "This is a jQuery plugin that takes an image selector as an argument having a SVG as source. Then it inlines the SVG so that the SVG stroke and path can be manipulated using plain CSS.",
  "main": "./jquery.inlinesvg.js",
  "keywords": [
    "browser",
    "jquery",
    "svg",
    "css",
    "inline",
    "image",
    "img"
  ],
  "license": "MIT",
  "ignore": [
    "**/.*",
    "bower_components",
    "README.md",
    ".editorconfig"
  ],
  "dependencies": {
    "jquery": ">=1.11"
  }
}
